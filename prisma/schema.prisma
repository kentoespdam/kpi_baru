// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Account {
  id     String @id
  userId String @unique
  name   String
  email  String @unique
  prefs  String
}

model Session {
  accessToken  String @id
  refreshToken String
}
